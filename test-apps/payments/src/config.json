{
	"author": "nobody@example.com",
	"config_version": "2",
	"description": "My Extension Description",
	"modules": {
		"activations": [
			{
				"patterns": [
					"http://*/*",
					"https://*/*"
				],
				"run_at": "start",
				"scripts": [
					"js/every-page.js"
				],
				"styles": []
			}
		],
		"background": {
			"files": [
				"js/background.js"
			]
		},
		"button": {
			"default_icon": "img/logo-forge.png",
			"default_popup": "index.html",
			"default_title": "An example browser toolbar icon"
		},
		"contact": true,
		"event": true,
		"file": true,
		"geolocation": true,
		"is": true,
		"logging": {
			"level": "INFO"
		},
		"message": true,
		"notification": true,
		"package_names": {
			"android": "io.trigger.test.payments",
			"ios": "io.trigger.test.payments"
		},
		"payments": {
			"androidPublicKey": ""
		},
		"prefs": true,
		"request": {
			"permissions": []
		},
		"sms": true,
		"tabs": true,
		"tools": true
	},
	"name": "CD test payments",
	"platform_version": "default",
	"version": "0.1"
}
